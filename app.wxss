/**app.wxss**/
 @import 'weui.wxss'; 
.page{
    font-family: PingFangSC-Regular;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.line{
    height: 8px;
    width: 100%;
    background: #F5F5F5;
}

.clearfix{
    clear:both;
}

/*联系客服*/
.contact{
    position: absolute;
    bottom: 30px;
    left: 0;
    width: 80%;
    height: 35px;
    line-height: 35px;
    padding: 0 10%;
}
.contact text{
    font-family: PingFangSC-Regular;
    font-size: 13px;
    color: #1890FF;
    letter-spacing: -0.01px;
    padding: 5px 20px;
}
.contact button{
    font-family: PingFangSC-Regular;
    font-size: 13px;
    color: #1890FF;
    letter-spacing: -0.01px;
    padding: 5px 20px;
    display: inline;
}
button{
    border:none;
    background: none;
}
button:active{
    background: #fff;
}
button:after{
    border: none;
}

.ellipsis-1 {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.ellipsis-2 {
	display: -webkit-box;
	overflow: hidden;
	text-overflow: ellipsis;
	word-wrap: break-word;
	word-break: break-all;
	white-space: normal !important;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}